{
    "name": "spryker/mini-api-framework",
    "minimum-stability": "dev",
    "require": {
      "spryker/glue-backend-api-application": "^0.2.0",
      "spryker/glue-application": "^1.39.0",
      "spryker/glue-http": "^0.2.0",
      "spryker/glue-rest-api-convention": "^0.1.0",
      "spryker/console": "^4.10.2",
      "spryker/install": "^1.0.0",
        "spryker/code-sniffer": "dev-master"
    },
    "require-dev": {
      "spryker/development": "^3.32.1"
    },
    "autoload": {
      "psr-4": {
        "Generated\\": "src/Generated/",
        "Orm\\": "src/Orm/",
        "Pyz\\": "src/Pyz/",
        "SprykerConfig\\": "src/SprykerConfig/"
      }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
